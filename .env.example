# Environment Variables for Gianna

# Default Language Model (openai, google, nvidia, groq, ollama)
DEFAULT_MODEL=openai

# API Keys
OPENAI_API_KEY=your_openai_api_key
GOOGLE_API_KEY=your_google_api_key
NVIDIA_API_KEY=your_nvidia_api_key
GROQ_API_KEY=your_groq_api_key
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Model Settings
OPENAI_MODEL=gpt-4-32k
GOOGLE_MODEL=gemini-pro
GROQ_MODEL=llama2-70b-chat
NVIDIA_MODEL=mistralai/mixtral-8x7b-instruct-v0.1

# Audio Settings
DEFAULT_TTS=google  # Options: elevenlabs, google, whisper
DEFAULT_STT=whisper_local  # Options: whisper_local, whisper_api

# Logging
LOGURU_LEVEL=INFO